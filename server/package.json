{
    "name": "policy-analyzer-server",
    "private": true,
    "version": "0.1.0",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "build": "tsc -p .",
        "start": "node dist/index.js || node dist/src/index.js",
        "postinstall": "prisma generate",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev --name init",
        "prisma:deploy": "prisma migrate deploy",
        "seed:baselines": "npm run build && node dist/scripts/seed-baselines.js"
    },
    "dependencies": {
        "@prisma/client": "^5.18.0",
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "dotenv": "^17.2.3",
        "groq-sdk": "^0.5.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "prisma": "^5.18.0",
        "typescript": "^5.4.0",
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17"
    }
}
